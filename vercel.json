{
  "buildCommand": "ng build --configuration production",
  "outputDirectory": "dist/receptichkaBg-client/browser",
  "git": {
    "deploymentEnabled": {
      "master": true
    }
  },
  "github": {
    "silent": true
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}